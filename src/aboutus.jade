			.row
				.col-xs-12.mobile-footer
					a(href='javascript:void(0);').mobile-footer__link
						img(src='img/Logo.svg' alt='Mobike Bazaar')
					a(href='javascript:void(0);').mobile-footer__link terms &amp; conditions

				.col-sm-12.col-md-10.col-md-offset-1.hidden-xs
					.col-sm-3.footer__bloc
						h4.footer__header.heading-sm information
						ul.footer-list
							li.footer-list__item
								a(href='aboutus.html').footer-list__link about us
							li.footer-list__item
								a(href='javascript:void(0);').footer-list__link delivery information
							li.footer-list__item
								a(href='privacy-policy.html').footer-list__link privacy policy
							li.footer-list__item
								a(href='terms-conditions.html').footer-list__link terms &amp; conditions
							li.footer-list__item
								a(href='javascript:void(0);').footer-list__link site map

					.col-sm-3.footer__bloc
						h4.footer__header.heading-sm my account
						ul.footer-list
							li.footer-list__item
								a(href='javascript:void(0);').footer-list__link my account
							li.footer-list__item
								a(href='javascript:void(0);').footer-list__link order history
							li.footer-list__item
								a(href='javascript:void(0);').footer-list__link wish list
							li.footer-list__item
								a(href='javascript:void(0);').footer-list__link newsletter
							li.footer-list__item
								a(href='javascript:void(0);').footer-list__link email a friend

					.col-sm-3.footer__bloc
						h4.footer__header.heading-sm service &amp; support
						ul.footer-list
							li.footer-list__item
								a(href='javascript:void(0);').footer-list__link brands
							li.footer-list__item
								a(href='javascript:void(0);').footer-list__link get vouchers
							li.footer-list__item
								a(href='javascript:void(0);').footer-list__link affiliates
							li.footer-list__item
								a(href='javascript:void(0);').footer-list__link get a quote

					.col-sm-3.footer__bloc
						h4.footer__header.heading-sm connect with us
						ul.footer-list
							li.footer-list__item
								a(href='tel:+91 9414111060').footer-list__link +91 9414111060
							li.footer-list__item
								a(href='mailto:sellers@mobikebazar.com').footer-list__link sellers@mobikebazar.com
						ul.social
							li.social__item
								a(href='javascript:void(0);').social__link.social__link--facebook
									i.mb-icon-facebook.social__icon
							li.social__item
								a(href='javascript:void(0);').social__link.social__link--twitter
									i.mb-icon-twitter.social__icon
							li.social__item
								a(href='javascript:void(0);').social__link.social__link--linkedin
									i.mb-icon-linkedin.social__icon
	//START: Login Modal- User
	#userLogin.modal.fade.modal--fullxs.modal--loginSignup(tabindex='-1', role='dialog')
		.modal-dialog(role='document')
			.modal-content
				.login-recovery-wrapper
					.login-wrapper
						.modal-header
							button.close(type='button', data-dismiss='modal', aria-label='Close')
								span(aria-hidden='true') ×
							h4.modal-title Login with MobikeBazar 
						.modal-body
							form.login-form(data-toggle="validator" role="form")
								.row
									.col-sm-12
										.form-group
											span.input.input--hoshi
												input#email-field.input__field.input__field--hoshi(type='text' data-error="Please enter email address" required)
												label.input__label.input__label--hoshi.input__label--hoshi-color-1(for='email-field')
													span.input__label-content.input__label-content--hoshi Email 
											.help-block.with-errors
									.col-sm-12
										.form-group
											span.input.input--hoshi
												input#password-field.input__field.input__field--hoshi(type='password' data-error="Please enter password" required)
												label.input__label.input__label--hoshi.input__label--hoshi-color-1(for='password-field')
													span.input__label-content.input__label-content--hoshi Choose Password
											.help-block.with-errors

									.col-sm-12.form-button
										button.button.button--primary-white.button--lg.btn-block LOGIN
									.col-sm-12.form-button
										.form-group.clearfix
											.form-bottom-link 
												a.button.button--link.recover-pw-link(href='#')  Recover Password
											.form-bottom-link New to mobike bazar? 
												a.button.button--link(href="http://projects.lollypop.biz/mobikebazar/signup.html")  Sign Up !
					.recover-pw-wrapper
						.modal-header
							h4.modal-title Recover Password 
							p.modal-subtitle We will send you a recovery mail. Please enter your email ID
						.modal-body
							form.login-form(data-toggle="validator" role="form")
								.row
									.col-sm-12
										.form-group
											span.input.input--hoshi
												input#email-field.input__field.input__field--hoshi(type='text' data-error="Please enter email address" required)
												label.input__label.input__label--hoshi.input__label--hoshi-color-1(for='email-field')
													span.input__label-content.input__label-content--hoshi Email
											.help-block.with-errors
								
									.col-sm-12.form-button
										button.button.button--primary-white.button--lg.btn-block recover password
									.col-sm-12.form-button
										.form-group.clearfix
											.text-center
												a.button.button--link.login-go-back(href='#')  LOGIN
	//START: Plugin Scripts
	script(src="js/plugins/jquery.js")
	script(src="js/plugins/bootstrap.min.js")
	script(src="js/plugins/vivus.min.js")

	//START: Custom Scripts
	script(src="js/vivus-init.js")
	script(src="js/main.js")
